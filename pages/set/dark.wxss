
.dark .set {
  background: #1e1f25;
  color: #8a8b8e;
  transition: all ease .3s;
}
.dark .header .header-clear {
  background: #27282e;
  transition: all ease .3s;
}

.dark .header .userinfo .userinfo-avatar {
  border: 3px solid white;
}

.dark .header .userinfo {
  background: #27282e;
  box-shadow: 0 4rpx 20rpx 0 rgba(0, 0, 0, .3);
}
.dark .header .userinfo .expires_time {
  background: transparent;
}
.dark .model {
  background: #27282e;
  box-shadow: 0 4rpx 20rpx 0 rgba(0, 0, 0, .3);
}

.dark .show-content {
  background: #27282e;
  color: #8a8b8e;
  transition: all ease .3s;
}