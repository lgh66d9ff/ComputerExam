.dark .exampage {
  background: #1e1f25;
  color: #8a8b8e;
}

.dark .exam-relate {
  background: #27282e;
  color: #8a8b8e;
  box-shadow: 0 4rpx 20rpx 0 rgba(0, 0, 0, .3);
}
.dark .exam-card:nth-child(2) {
  background: linear-gradient(to right, #0470a0, #5c8ca2);
}
.dark .exam-card:nth-child(3) {
  background: linear-gradient(to right, #a0602a, #af8d71);
}
.dark picker .exam-card {
  background: linear-gradient(to right, #923b37, #987b79);
}